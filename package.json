{
  "name": "langgraph-python-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:agent\" --names ui,agent --prefix-colors blue,green --kill-others",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "dev:agent": "cd agent && npx @langchain/langgraph-cli dev --port 8123 --no-browser --allow-blocking",
    "dev:ui": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "PLAYWRIGHT_BROWSERS_PATH=0 playwright test --config tests/playwright.config.ts",
    "test:e2e:ui": "PLAYWRIGHT_BROWSERS_PATH=0 playwright test --config tests/playwright.config.ts --ui",
    "test:e2e:headed": "PLAYWRIGHT_BROWSERS_PATH=0 playwright test --config tests/playwright.config.ts --headed",
    "test:e2e:update-snapshots": "PLAYWRIGHT_BROWSERS_PATH=0 playwright test --config tests/playwright.config.ts --update-snapshots",
    "test:all": "npm run test && npm run test:e2e",
    "install:agent": "sh ./scripts/setup-agent.sh || scripts\\setup-agent.bat",
    "postinstall": "npm run install:agent"
  },
  "dependencies": {
    "@copilotkit/react-core": "1.51.3",
    "@copilotkit/react-ui": "1.51.3",
    "@copilotkit/runtime": "1.51.3",
    "@copilotkit/runtime-client-gql": "1.51.3",
    "next": "16.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react18-json-view": "^0.2.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@langchain/langgraph-cli": "^1.0.4",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
